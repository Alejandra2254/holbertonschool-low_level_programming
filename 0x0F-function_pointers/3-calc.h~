#ifndef CALC_H
#define CALC_H
#include <stdio.h>
/**
 * struct op - Struct op
 *
 * @op: The operator
 * @f: The function associated
 */
typedef struct op /** la estructura como primer argumento el operador*/
/** el segundo el puntero a funcion */
{
	char *op;
	int (*f)(int a, int b);
} op_t;

int op_add(int a, int b); /** los prototipos de la funciones bases */
int op_sub(int a, int b);
int op_mul(int a, int b);
int op_div(int a, int b);
int op_mod(int a, int b);
int (*get_op_func(char *s))(int, int); /* prototipo de funcion que selecciona*/
/* la funcion correca */
#endif
