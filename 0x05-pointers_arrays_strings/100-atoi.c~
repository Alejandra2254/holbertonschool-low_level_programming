#include "holberton.h"
/**
 * _atoi - prints numbers
 * @s: string
 * Return: 0
 */
int _atoi(char *s)
{
	unsigned int i = 0, l = 0, first, finish, num, digit, sign, result;
	while (s[l] != '\0')
		l++;
	if (l == 0)
		return (0);
	for (i = 0; i < l; i++)
	{
		sign = 0;
		if (s[i] == '-')
			sign++;
		if (s[i] >= 48 && s[i] <= 57)
		{
			first = i;
			break;
		}
	}
	for (; i < lenght; i++)
	{
		if (s[i] < 48 || s[i] > 57)
		{
			break;
		}
	}
	finish = i - 1;
	for (; init <= finish; init++)
	{
		digito = s[init] - '0';
		num = 1;
		for (i = 0; i < finish - init ; i++)
		{
			num = num * 10;
		}
		num = num * digito;
		result = num + result;
	}
	if (sign % 2 == 1)
	{
		result = result * -1;
	}
	return (result);
}
